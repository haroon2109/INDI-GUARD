# IndiGuard Platform Configuration
# Rename this file to .env and fill in your keys

# OpenWeatherMap API Key (Optional for live weather)
OPENWEATHER_API_KEY=your_openweather_key_here

# Mapbox Token (Optional for custom tiles)
MAPBOX_TOKEN=your_mapbox_token_here

# Database URL (If connecting to external DB)
DATABASE_URL=sqlite:///data.db

# Debug Mode (True/False)
DEBUG=False
